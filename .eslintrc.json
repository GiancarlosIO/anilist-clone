{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "es5",
        "bracketSpacing": true,
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "arrowParens": "always"
      }
    ]
  }
}
